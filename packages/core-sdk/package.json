	},
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"homepage": "https://github.com/koyofinance/sdk/tree/main/packages/core-sdk",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/koyofinance/sdk.git",
