{
	"name": "@koyofinance/swap-sdk",
	"version": "0.3.0",
	"description": "",
	"author": "@koyofinance",
	"license": "UNLICENSED",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"@koyofinance/core-sdk",
		"koyo",
		"koyo-finance",
		"koyofinance",
		"sdk",
		"swap",
		"pools"
	],
	"scripts": {
		"lint": "eslint src tests --ext ts --fix -c ../../.eslintrc",
		"build": "tsup && tsc -b src",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"generate:typechain:stableswaps": "typechain --target ethers-v5 --always-generate-overloads --discriminate-types --out-dir ./src/types/contracts/stableswap './src/constants/abis/stableswap/*.json'",
		"generate": "yarn generate:typechain:stableswaps"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"dependencies": {
		"@koyofinance/core-sdk": "workspace:^",
		"@koyofinance/react-query-typechain": "workspace:^",
		"ethers": "^5.6.8",
		"react-query": "^3.38.1",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@typechain/ethers-v5": "^10.1.0",
		"@types/react": "17.0.39",
		"@types/react-dom": "17.0.11",
		"react": "^17.0.2",
		"react-dom": "17.0.2",
		"tsup": "^5.12.7",
		"typechain": "^8.1.0",
		"typescript": "^4.6.4"
	},
	"resolutions": {
		"@types/react": "17.0.39",
		"@types/react-dom": "17.0.11"
	},
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"homepage": "https://github.com/koyo-finance/sdk/tree/main/packages/swap-sdk",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/koyo-finance/sdk.git",
		"directory": "packages/swap-sdk"
	},
	"bugs": {
		"url": "https://github.com/koyo-finance/sdk/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
